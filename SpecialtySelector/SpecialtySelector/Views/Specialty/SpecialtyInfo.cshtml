@model IEnumerable<SpecialtySelector.Models.Specialties.SpecialtyInfo>



@if (Model.Count() != 0)
{
    foreach (var x in Model)
    {
        <div>
            @x.Name
        </div>
        <div>
            @x.Description
        </div>
        <div>
            @x.Eqd
        </div>
        <div>
            @x.FormOfEducation
        </div>
        <div>
            @x.SubDepartmentId
        </div>
    }
}
else
{
    <h1 class="text-center lead">Все още няма добавени поднаправления къ съответното направление ....</h1>
}

